<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="#6B2BAA" />
            <stroke android:width="2dp" android:color="#5A2491" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="oval">
            <solid android:color="#9D4FFF" />
            <stroke android:width="2dp" android:color="#8338ec" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="#A463FF" />
            <stroke android:width="1dp" android:color="#8338ec" />
        </shape>
    </item>
</selector>
